{
  "name": "react-stater",
  "version": "1.0.0",
  "description": "React starter boilerplate",
  "main": "index.js",
  "author": "Dibyendu Das",
  "license": "MIT",
  "scripts": {
    "dev": "npm run prebuild && cross-env NODE_ENV=development webpack serve",
    "dev:bundleanalyzer": "npm run prebuild && npm run dev --analyze",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack",
    "build:bundleanalyzer": "npm run build --analyze"
  },
  "keywords": [
    "react",
    "webpack 5",
    "webpack"
  ],
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.0",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.2",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.0",
    "postcss": "^8.4.19",
    "postcss-loader": "^7.0.1",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
